        <hr>

        <!-- Pagination -->
        <div class="row text-center">
            <div class="col-lg-12">
                <% if (pageCount > 1) { %>
                    <ul class="pagination">
                        <% if (currentPage > 1) { %>
                            <li><a href="/services?page=1">&laquo;</a></li>
                        <% } %>
                        <% var i = 1;
                        if (currentPage > 5) {
                            i = +currentPage - 4;
                        } %>
                        <% if (i !== 1) { %>
                            <li class="disabled"><a href="#">...</a></li>
                        <% } %>
                        <% for (i; i<=pageCount; i++) { %>
                            <% if (currentPage == i) { %>
                                <li class="active"><span><%= i %> <span class="sr-only">(current)</span></span></li>
                            <% } else { %>
                                <li><a href="/services?page=<%= i %>"><%= i %></a></li>
                            <% } %>
                            <% if (i == (+currentPage + 4)) { %>
                                <li class="disabled"><a href="#">...</a></li>
                            <% break; } %>
                        <% } %>
                        <% if (currentPage != pageCount) { %>
                            <li><a href="/services?page=<%= pageCount %>">&raquo;</a></li>
                        <% } %>
                    </ul>
                <% } %>
            </div>
        </div>
